 * https://blog.csdn.net/humphreyandkate/article/details/104731073
 *
 * Clock phase CPHA should be 2 Edge
 *
 * space < 4M/bit
 */
#include "main.h"
#include "spi.h"
#include "gpio.h"

#define SS 0x1
#define MUX 0x4
#define PGA 0x0
#define MODE 0x1

#define DR 0x4
#define TS_MODE 0x0
#define PULLUP_EN 0x1
#define NOP 0x1

void ADS1118_init(void);
int ADS1118_GetData(void);
